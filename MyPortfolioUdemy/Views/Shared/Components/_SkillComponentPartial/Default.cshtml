@model List<MyPortfolioUdemy.DAL.Entities.Skill>

<div class="col-six tab-full right">
    <h3>Programlama Dilleri & Teknolojiler</h3>

    @if (Model != null && Model.Any())
    {
        <table class="skills-table">
            <thead>
                <tr>
                    <th>Beceriler</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Title</td>
                    </tr>
                }
            </tbody>
        </table>
    }
    else
    {
        <p>Henüz eklenmiş bir beceri yok.</p>
    }
</div>
